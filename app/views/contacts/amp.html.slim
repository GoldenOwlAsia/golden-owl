    <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>
  body
    / .header-section
      / .navigation
      /   = link_to root_path do
      /     .logo
      /       .image-logo
      /         / = image_tag 'logo_new.png', :alt => "Golden Owl Consulting"
      /         amp-img alt=("Golden Owl Consulting") height="50" layout="responsive" src="/assets/logo_new.png" width="50"

      /   end
        .navbar
          / = image_tag 'iconbar.png'
          / amp-img alt=("Golden Owl Consulting") height="50" layout="responsive" src="/assets/iconbar.png" width="50" style="display: inline-block;"
    .slider-section
      .slider-content
        h2.slider-title
          |  Disruptive Thinking Makes
          br/
          span  Real World Technology Solutions
        button.btn-details FULL VIEW
        button.btn-customs
          i.fa.fa-angle-down

    .service-section
      h3.title-section
        span OUR SERVICES
        /.line-title
      h4.sub-title-section We are experts in IT field, we believe to bring you best solutions for your ideas.
      .service-content.container-fluid
        .service-element
          .image-service[id="web-img"]
            / = image_tag 'icon-outsource.png', :width => '80px', :height => '65%', :style => 'margin-top: 17.5%', :class => 'img-web'
            amp-img height="1" layout="responsive" src="/assets/icon-outsource-0a2890c7f999087398dc3730f7fa5c208091757429a7734a84c0afc66808c916.png" width="1"
            / style="height: 65%; width: 80px; margin-top: 17.5%;"
          h5.title-service Web development

        .service-element
          .image-service[id="mobi-img"]
            / = image_tag 'icon-mobile.png', :width => '45%', :height => '45%', :style => 'margin-top: 30%'

            amp-img height="1" layout="responsive" src="/assets/icon-mobile.png" width="1"
            / style="margin-top: 30%; height: 45%; width: 45%;"
          h5.title-service Mobile development

        / .service-element
        /   .image-service
        /     = image_tag 'icon-seo.png', :width => '45%', :height => '45%', :style => 'margin-top: 27%'
        /   h5.title-service Seo

        .service-element
          .image-service[id="design-img"]
            / = image_tag 'icon-design.png', :width => '50%', :height => '45%', :style => 'margin-top: 28%'
            amp-img height="1" layout="responsive" src="/assets/icon-design.png" width="1"
            / style="margin-top: 28%; height: 45%; width: 50%;"
          h5.title-service Design

        .service-element
          .image-service[id="pm-img"]
            / = image_tag 'icon-pm.png', :width => '40%', :height => '50%', :style => 'margin-top: 22.5%'
            amp-img height="1" layout="responsive" src="/assets/icon-pm.png" width="1"
            / style="margin-top: 30%; height: 40%; width: 50%;"
          h5.title-service Project Management

        .service-element
          .image-service[id="hr-img"]
            / = image_tag 'icon-hr.png', :width => '55%', :height => '55%', :style => 'margin-top: 24%'
            amp-img height="1" layout="responsive" src="/assets/icon-hr.png" width="1"
            / style="margin-top: 24%; height: 55%; width: 55%;"
          h5.title-service Tech Staffing

      button.btn-more
        = link_to root_path
          i.fa.fa-angle-double-down


