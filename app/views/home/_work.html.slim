        h6 Serumi
      #luxify.project-element
        i.fa.fa-diamond
        h6 Luxify
      #slc.project-element
        i.fa.fa-graduation-cap
        h6 SLC
      #cpd.project-element
        i.fa.fa-search-plus
        h6 CPD
      #artfolo.project-element
        i.fa.fa-picture-o
        h6 Artfolo
    button.btn-right 
      i.fa.fa-chevron-circle-right
